{
  "name": "ug-backend",
  "version": "1.0.0",
  "description": "UG管理系统后端API服务",
  "private": true,
  "main": "app.js",
  "type": "commonjs",
  "scripts": {
    "start": "egg-scripts start --daemon --title=ug-backend",
    "stop": "egg-scripts stop --title=ug-backend",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "pnpm lint -- --fix && pnpm test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:strict": "eslint . --max-warnings 0",
    "ci": "pnpm lint && pnpm cov",
    "autod": "autod",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "spellcheck": "node ../node_modules/.pnpm/cspell@9.2.1/node_modules/cspell/bin.mjs lint app/**/*"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "dayjs": "^1.11.9",
    "egg": "3.31.0",
    "egg-cors": "^3.0.1",
    "egg-jwt": "^3.1.7",
    "egg-scripts": "^3.1.0",
    "egg-sequelize": "^6.0.0",
    "egg-validate": "^2.0.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mysql2": "^3.6.0",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "uuid": "^12.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cspell": "^9.2.1",
    "egg-bin": "^6.13.0",
    "egg-ci": "^2.1.0",
    "egg-mock": "^5.10.0",
    "eslint": "^8.0.0",
    "eslint-config-egg": "^14.1.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": "^20.19.0"
  },
  "overrides": {
    "undici": "^5.28.4"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "UG Team",
  "license": "MIT",
  "packageManager": "pnpm@8.15.8"
}