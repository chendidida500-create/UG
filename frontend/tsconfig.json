{
  "extends": "../config/tsconfig/frontend.json",
  "compilerOptions": {
    "ignoreDeprecations": "5.0",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types",
      "../node_modules/@types",
      "../../node_modules/@types"
    ],
    "types": [
      "node",
      "jest"
    ],
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  },
  "include": [
    "src/**/*",
    ".umirc.ts"
  ]
}