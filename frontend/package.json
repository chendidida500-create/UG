  "description": "UG管理系统前端应用",
  "private": true,
  "scripts": {
    "build": "umi build",
    "dev": "umi dev",
    "format": "prettier --cache --write .",
    "postinstall": "umi setup",
    "setup": "umi setup",
    "start": "npm run dev",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
