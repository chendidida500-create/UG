{
  "name": "ug",
  "version": "1.0.0",
  "description": "UG管理系统 - 基于UMI + Egg.js的现代化后台管理系统",
  "private": true,
  "workspaces": {
    "packages": [
      "frontend",
      "backend"
    ]
  },
  "scripts": {
    "postinstall": "npm run setup",
    "setup": "yarn setup:frontend && yarn setup:backend",
    "setup:frontend": "cd frontend && yarn install",
    "setup:backend": "cd backend && yarn install",
    "dev": "concurrently \"yarn dev:backend\" \"yarn dev:frontend\"",
    "dev:backend": "cd backend && npx egg-bin dev",
    "dev:frontend": "cd frontend && yarn dev",
    "build": "yarn build:frontend",
    "build:frontend": "cd frontend && yarn build",
    "test": "concurrently \"yarn test:backend\" \"yarn test:frontend\"",
    "test:backend": "cd backend && yarn test",
    "test:frontend": "cd frontend && yarn test",
    "lint": "concurrently \"yarn lint:backend\" \"yarn lint:frontend\"",
    "lint:backend": "cd backend && yarn lint",
    "lint:frontend": "cd frontend && yarn lint",
    "clean": "concurrently \"yarn clean:backend\" \"yarn clean:frontend\"",
    "clean:backend": "cd backend && rimraf node_modules",
    "clean:frontend": "cd frontend && rimraf node_modules",
    "db:migrate": "cd backend && yarn db:migrate",
    "db:seed": "cd backend && yarn db:seed",
    "start": "concurrently \"yarn start:backend\" \"yarn start:frontend\"",
    "start:backend": "cd backend && yarn start",
    "start:frontend": "cd frontend && yarn start"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "concurrently": "^8.2.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": "20.14.0"
  },
  "resolutions": {
    "undici": "^5.28.4"
  },
  "overrides": {
    "undici": "^5.28.4"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "umi",
    "egg.js",
    "react",
    "typescript",
    "antd",
    "admin",
    "rbac"
  ],
  "author": "UG Team",
  "license": "MIT",
  "packageManager": "yarn@1.22.22"
}
